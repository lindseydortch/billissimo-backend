model Account {
  id                        Int         @id @default(autoincrement())
  bank_name                 String
  account_type              accountType
  direct_deposit_allocation Int

  // Foreign Relationships
  bill_id Int
  bill    Bill?
  user_id Int
  user    User  @relation(fields: [user_id], references: [id])
}

enum accountType {
  Checking
  Savings
}
